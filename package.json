{
  "name": "converter",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "depcheck": "manypkg check",
    "depfix": "manypkg fix"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@manypkg/cli": "^0.19.2",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "packageManager": "npm@8.19.2"
}
